CREATE TABLE guestChecks (
    guestCheckId BIGINT PRIMARY KEY,
    chkNum INT,
    opnBusDt DATE,
    opnUTC DATETIME,
    opnLcl DATETIME,
    clsdBusDt DATE,
    clsdUTC DATETIME,
    clsdLcl DATETIME,
    lastTransUTC DATETIME,
    lastTransLcl DATETIME,
    lastUpdatedUTC DATETIME,
    lastUpdatedLcl DATETIME,
    clsdFlag BOOLEAN,
    gstCnt INT,
    subTtl DECIMAL(10, 2),
    nonTxblSlsTtl DECIMAL(10, 2),
    chkTtl DECIMAL(10, 2),
    dscTtl DECIMAL(10, 2),
    payTtl DECIMAL(10, 2),
    balDueTtl DECIMAL(10, 2),
    rvcNum INT,
    otNum INT,
    ocNum INT,
    tblNum INT,
    tblName VARCHAR(50),
    empNum INT,
    numSrvcRd INT,
    numChkPrntd INT
);
